{
    "name": "weprovide/valet-plus",
    "description": "Blazing fast macOS PHP development environment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mischa Braam",
            "email": "mischa.braam@iodigital.com"
        }
    ],
    "autoload": {
        "files": [
            "cli/includes/facades.php"
        ],
        "psr-4": {
            "WeProvide\\ValetPlus\\": "cli/ValetPlus/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    },
    "bin": [
        "valet-plus"
    ],
    "require": {
        "php": "^7.1|^8.0",
        "laravel/valet": "^4.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpro/grumphp-shim": "^2.4"
    }
}
